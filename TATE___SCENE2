if LocalPlayer.Character then

    for i,v in pairs(game.ReplicatedStorage.OrchestraScenes[SceneName].Environment.PlayerSpawns:GetChildren()) do

        if v:IsA("BasePart") then


            LocalPlayer.Character:MoveTo(v.Position)
            print("teleported player")
        else
            warn("couldnt teleport player")
        end
    end
end
print(game.Players.LocalPlayer.." was here")
