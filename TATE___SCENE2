local SceneName = "Song2"

local rep = game:GetService("ReplicatedStorage")
local songs = rep.OrchestraScenes
local song1 = songs[SceneName]


game.Lighting:ClearAllChildren()

if game:GetService("Workspace"):FindFirstChild("ScenesClient") then

    game:GetService("Workspace").ScenesClient:Destroy()
    print("destroyed old client")
    game:GetService("Workspace").ScenesServer:Destroy()
    print("destroyed old server")

elseif game:GetService("Workspace"):FindFirstChild("Client") then
    game:GetService("Workspace").Client:Destroy()
    print("destroyed old client")
    game:GetService("Workspace").Server:Destroy()
    print("destroyed old server")
end

game:GetService("Workspace").Terrain:Clear()
print("cleared terrain")

for i, v in ipairs(song1.Environment.Lighting:GetChildren()) do
    local clone = v:Clone()
    clone.Parent = game:GetService("Lighting")
    print("loaded "..v.Name)
end
print("loaded lighting")

song1.Environment.Client:Clone().Parent = game:GetService("Workspace")
print("added scene client")
song1.Environment.Server:Clone().Parent = game:GetService("Workspace")
print("added scene server")


local LocalPlayer = game:GetService("Players").LocalPlayer

